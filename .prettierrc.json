{
    "semi": false,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "printWidth": 100,
    "vueIndentScriptAndStyle": true,
    "singleAttributePerLine": true,
    "bracketSameLine": true
}
